{
	"name": "serverpod",
	"displayName": "Serverpod",
	"description": "This extension helps you in creating a serverpod project.",
	"version": "0.0.1",
	"publisher": "minnu1031",
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "https://github.com/yahu1031/serverpod_ext"
	},
	"icon": "assets/serverpod.png",
	"engines": {
		"vscode": "^1.66.x"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:serverpod.serverpod"
	],
	"main": "./out/index.js",
	"contributes": {
		"commands": [
			{
				"command": "serverpod.serverpod",
				"title": "Serverpod"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}